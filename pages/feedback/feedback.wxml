<!--pages/feedback/feedback.wxml-->
<view class='feedback'>
  <view class='title'>反馈内容 <text>*</text></view>
  <view class="section">
    <textarea bindblur="bindTextAreaBlur" placeholder="请填写您的建议" />
  </view>
  <view class='title'>手机号码 <text>*</text></view>
  <view class="section">
    <input  maxlength="11" placeholder="请填写您的手机号" type="number"/>
  </view>
   <view class='all_btn'>
    <button type="warn" bindtap="warn">提交</button>
  </view>
</view>