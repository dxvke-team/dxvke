<!--pages/superBenefit/superBenefit.wxml-->
<view class='superBenefit'>
  <view>
    <view>
      <image src='{{nine_banner.image}}' class="nav"></image>
    </view>
    <view class='main'>
      <view class='main_top'>
        <image src='../../images/superBenefit/sanjiao.png' class='sanjiao'></image>
        <view data-type='{{nine_banner.type_id}}' catchtap="toSubject" >
          <image src='../../images/superBenefit/youhui_bd_img.png' class='title_img'></image>
          <text class='title'>9.9专区</text>
          <view class='right'>
            <text>进入专场</text>
            <image src='../../images/superBenefit/gt_white.png'></image>
          </view>
        </view>
      </view>
      <view class='scroll_box'>
        <scroll-view scroll-x="true" style="width: auto;overflow:hidden;">
          <view class="item_list" wx:for="{{list}}" wx:key="{{list}}">
            <image src="../../images/product.png" class="item_list_img" mode="aspectFill"></image>
            <view class="item_list_text">
              <text class='item_list_name break'>商品名称商品免除棉城名称名称棉城圣诞节搜束带结发ID时代峰峻搜房</text>
              <text class='item_list_newPrice'>￥9<text>.9</text></text>
              <text class='item_list_oldPrice'>10<text>.1</text></text>
            </view>
          </view>
        </scroll-view>
      </view>
    </view>
  </view>
