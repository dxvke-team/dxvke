<!--pages/share/share.wxml-->
<view class='share'>
   <view class='main'>
      <textarea placeholder="说说你的晒单感受吧~"/>
      <view>
         <view style='display:inline-block;position:relative;' wx:for="{{imgs}}" wx:key="{{imgs}}">
           <image class='img' src='imgs'></image>
            <image class='del' src='../../images/cancel_img.png'></image> 
         </view>
         <image catchtap='chooseImageTap' src='../../images/upload.png' class='img'></image>
      </view>
   </view>
   <view>
       <view class='order-num'>订单号<input placeholder='请输入订单号'></input></view>
   </view>
   <view class='bottom'>
        <view class='tip'>
          <image src='../../images/litter_sanjiao.png' class='icon'></image>晒单说明
        </view>
        <view class='brief'>晒单说明说哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈米昂说明说米昂说说覅大当家斯蒂芬o</view>
   </view>
  <view style='height:140rpx;'></view>
  <view class='all_btn' catchtap='toShare'>
    <button type="warn" bindtap="warn">发布</button>
  </view>
</view>